<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.gittest.common.CheckMapper">

  <select id = "getUserId">
      SELECT user_id
      from user
      where user_id= #{userId}
  </select>


<select id = "getPetId">
    SELECT pet_id
    from pet
    where pet_id= #{petId}
</select>


</mapper>